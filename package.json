{
  "name": "parcel-riot-mocktimes-starter",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "dist": "run-s dist:*",
    "dist:clean": "rimraf dist",
    "dist:build": "parcel build src/index.html --public-url=./",
    "dist-dev": "run-s dist-dev:*",
    "dist-dev:clean": "rimraf dist-no-minified",
    "dist-dev:build": "parcel build src/index.html --out-dir=dist-no-minified --no-minify --public-url=./",
    "lint": "standard --verbose | snazzy",
    "precommit": "lint-staged",
    "start": "am-mocktimes watch",
    "screenshot": "am-mocktimes--screenshot",
    "storybook": "start-storybook -p 6006 -s ./src",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-backgrounds": "^4.0.11",
    "@storybook/addon-knobs": "^4.0.6",
    "@storybook/addon-storysource": "^4.0.11",
    "@storybook/addon-viewport": "^4.0.6",
    "@storybook/addons": "^4.0.6",
    "@storybook/riot": "^6.2.9",
    "am-mocktimes": "^2.0.1",
    "am-mocktimes-addon-screenshot": "0.0.2",
    "ava": "^2.4.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-loader": "^8.0.4",
    "esm": "^3.2.25",
    "husky": "^0.14.3",
    "less": "^3.0.4",
    "lint-staged": "^7.1.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.12.2",
    "parcel-plugin-tag": "^1.1.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "riot-tag-loader": "^2.1.0",
    "snazzy": "^7.1.1",
    "standard": "^10.0.3"
  },
  "ava": {
    "files": [
      "src/states/**/*.test.js"
    ],
    "require": [
      "esm"
    ]
  },
  "standard": {
    "globals": [
      "APP"
    ]
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "dob": "^2.5.8",
    "normalize.css": "^8.0.0",
    "riot": "^3.10.1",
    "riot-route": "^3.1.4"
  }
}
